-- bullets

function shoot() -- this needs heavy reworking
	local dx=ship.bdx
	local dy=ship.bdy
	if btnp(5) and time()>=shoot_time then
		shoot_time=time()+ship.shoot_wait
		make_bullet(ship.x,ship.y,dx,dy)
	end
end

function make_bullet(x,y,dx,dy)
	local thing={
	x=x,
	y=y,
	hbx=3,
	hby=3,
	hbsx=4,
	hbsy=4,
	sp=4,
	dx=dx,
	dy=dy,
	t=time()+4,
 }
 --sfx(6,1)
	add(bullets, thing)
end

function draw_bullets()
	foreach(bullets, function(bullet)
		spr(bullet.sp, bullet.x, bullet.y)
	end)
end

function update_bullets()
	foreach(bullets, function(bullet)
		bullet.x+=bullet.dx
		bullet.y+=bullet.dy
		local xoffset=0
		if bullet.dx>0 then xoffset=7 end
		local h=mget((bullet.x+xoffset)/8,(bullet.y+7)/8)
		if fget(h,0) or fget(h,7) then
			del(bullets,bullet)
		end
		if bullet.t<=time() then
			del(bullets,bullet)
		end 
	end)
end
