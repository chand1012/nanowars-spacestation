-- draw functions
function _draw()
	cls()
	if stage==1 then
		draw_stars()
		draw_player()
		draw_enemies()
		draw_bullets()
		draw_shield()
	else
		draw_stars()
		draw_player()
		draw_enemies()
		draw_bullets()
	end
	draw_booms()
	print(score,100,119,3)
	draw_lives()
end

function draw_stars()
 foreach(stars, function(star)
 	pset(star.x,star.y,7)
 end)
end

function draw_mini_station()
	rectfill(36,2,56,8,0)
	circfill(36,5,5,0)
	circfill(56,5,5,0)
end

-- object, starting frame, number of frames, animation speed, flip
function anim(o,sf,nf,sp,fl)
  if(not o.a_ct) o.a_ct=0
  if(not o.a_st) o.a_st=0
  o.a_ct+=1
  if(o.a_ct%(30/sp)==0) then
    o.a_st+=1
    if(o.a_st==nf) o.a_st=0
  end
  o.a_fr=sf+o.a_st
  spr(o.a_fr,o.x,o.y,1,1,fl)
end

function draw_player() -- make this so that you use all 3 sprites
	local angle=0
	if btn(1) then
		ship.right=true
		ship.bdx=5
	elseif btn(0) then
		ship.right=false
		ship.bdx=(-5)
	elseif (btn(2) or btn(3)) and (not(btn(1) or btn(0))) then
		ship.bdx=0
	end
	if btn(3) then
		ship.down=true
		ship.bdy=5
	elseif btn(2) then
		ship.down=false
		ship.bdy=(-5)
	elseif (btn(1) or btn(0)) and not(btn(2) or btn(3)) then
		ship.down=nil
		ship.bdy=0
	end
	
	if btn(2) and btn(1) then
		angle=45
	elseif btn(1) and btn(3) then
		angle=135
	elseif btn(0) and btn(3) then
		angle=225
	elseif btn(0) and btn(2) then
		angle=315
	end
	
	if ship.down==nil and angle==0 then
		spr(ship.osp,ship.x,ship.y,1,1,ship.right,false)
	elseif angle==0 then
		spr(ship.sp,ship.x,ship.y,1,1,false,ship.down)
	elseif angle==45 then
		spr(ship.asp,ship.x,ship.y,1,1,true,false)
	elseif angle==135 then
		spr(ship.asp,ship.x,ship.y,1,1,true,true)
	elseif angle==225 then
		spr(ship.asp,ship.x,ship.y,1,1,false,true)
	elseif angle==315 then
		spr(ship.asp,ship.x,ship.y,1,1,false,false)
	end
end

function draw_lives()
	local count=0
	for i=1,ship.lives do
		spr(21,15+count,117)
		count+=10
	end
end
