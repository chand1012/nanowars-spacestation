-- update functions
function _update()
	update_player()
	update_bullets()
end

function update_player()
	shoot()
	if btn(0) and ship.dx>(-ship.limit) then
		ship.dx+=ship.vel*(-1)
	elseif btn(1) and ship.dx<ship.limit then
		ship.dx+=ship.vel
	elseif ship.dx>ship.minx and not(btn(0) or btn(1)) then
		ship.dx-=ship.vel*0.5
	elseif ship.dx<(-ship.minx) and not(btn(0) or btn(1)) then
		ship.dx+=ship.vel*0.5
	end
	if btn(2) and ship.dy>(-ship.limit) then
		ship.dy+=ship.vel*(-1)
	elseif btn(3) and ship.dy<ship.limit then
		ship.dy+=ship.vel
	elseif ship.dy>(ship.miny) and not(btn(2) or btn(3)) then
		ship.dy-=ship.vel*0.5
	elseif ship.dy<(ship.miny) and not(btn(2) or btn(3)) then
		ship.dy+=ship.vel*0.5
	end
	ship.x+=ship.dx
	ship.y+=ship.dy
end


