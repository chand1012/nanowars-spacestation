function make_enemy(x,y,kind)
	local enemy={
		x=x,
		y=y,
		sx=x,
		sy=y,
		sp=34,
		dx=(-1),
		dy=0,
		hbx=0,--hitbox things
		hby=0,
		hbsx=8,
		hbsy=8,
		hp=1,
		shp=1,
		kind=kind
		}
		add(enemies, enemy)
end
