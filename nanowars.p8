--bullets

function shoot() -- this needs heavy reworking
	local dx=0
	local dy=0
	if btnp(5) and time()>=shoot_time then
		shoot_time=time()+ship.shoot_wait
		if btn(0) then
			dx=-5
			last=-5
		elseif btn(2) and (not btn(1)) then
			dx=0
		elseif btn(1) then
			dx=5 -- fix this
			last=5
		else
			if last==nil then
				dx=5
			else
				dx=last
			end
		end
		if btn(3) then
			dy=5
		elseif btn(2) then
			dy=-5
		else
			dy=0
		end
		make_bullet(ship.x,ship.y,dx,dy)
	end
end

function make_bullet(x,y,dx,dy)
	local thing={
	x=x,
	y=y,
	hbx=3,
	hby=3,
	hbsx=4,
	hbsy=4,
	sp=4,
	dx=dx,
	dy=dy
 }
 sfx(6,1)
	add(bullets, thing)
end

function draw_bullets()
	foreach(bullets, function(bullet)
		spr(bullet.sp, bullet.x, bullet.y)
	end)
end

function update_bullets()
	foreach(bullets, function(bullet)
		bullet.x+=bullet.dx
		bullet.y+=bullet.dy
		local xoffset=0
		if bullet.dx>0 then xoffset=7 end
		local h=mget((bullet.x+xoffset)/8,(bullet.y+7)/8)
		if fget(h,0) or fget(h,7) then
			del(bullets,bullet)
		end
		if bullet.x>ship.x+100 or bullet.x<ship.x-100 then
			del(bullets, bullet)
		end
	end)
end
