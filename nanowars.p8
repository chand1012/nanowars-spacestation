-- init functions
function _init()
	ship={ -- add hitbox
		x=0,
		y=0,
		dx=0,
		dy=0,
		minx=0.01,
		miny=0,
		angle=0,
		vel=1,
		limit=2.5,
		sp=17,
		osp=18,
		asp=19,
		right=true,
		down=nil,
		shoot_wait=0.25,
		hbx=0,
		hby=0,
		hbsx=8,
		hbsy=8,
		hp=10,
		lives=3,
	}
	enemies={}
	bullets={}
	holes={}
	stars={}
	shoot_time=0
	rnd_stars()
end

function rnd_stars()
	for i=1,100 do
		local star={x=rnd(127),y=rnd(127)}
		add(stars,star)
	end
end
