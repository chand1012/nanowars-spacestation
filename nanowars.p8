--misc functions
function destroy_all(things)
	foreach(things, function(thing)
		del(things, thing)
	end)
end

function collide(obj, other)
  if other.x+other.hbx+other.hbsx > obj.x+obj.hbx and
      other.y+other.hby+other.hbsy > obj.y+obj.hby and
      other.x+other.hbx < obj.x+obj.hbx+obj.hbsx and
      other.y+other.hby < obj.y+obj.hby+obj.hbsy then
    return true
  else
  	return false
  end
end

function make_boom(x,y,t)
 local b={
 	x=x,
 	y=y,
 	t=t+time(),
 	sp=5
 }
 add(booms,b)
end

function draw_booms()
	foreach(booms, function(boom)
		if boom.t<=time() then
			del(booms,boom)
		end
		anim(boom,boom.sp,3,10,false)
	end)
end


