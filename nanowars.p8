-- init functions

-- game states:
-- 0 = menu
-- 1 = main stage
-- 2 = hyperspace transition
-- 3 = space station stage
-- 4 = game over
function _init()
	stage=1 -- has to do with game states
	score=tonum(stat(6))
	if score==nil then
		score=0
	end
	ship={ -- add hitbox
		x=72,
		y=94,
		dx=0,
		dy=0,
		minx=0.01,
		miny=0,
		angle=0,
		vel=1,
		limit=2,
		sp=17,
		osp=18,
		asp=19,
		right=true,
		down=nil,
		shoot_wait=0.1,
		hbx=0,
		hby=0,
		hbsx=8,
		hbsy=8,
		hp=25,
		lives=3,
		bdx=5,
		bdy=0,
	}
	enemies={}
	bullets={}
	holes={}
	stars={}
	booms={}
	shield={}
	shoot_time=0
	rnd_stars()
	make_shield()
end

function rnd_stars()
	for i=1,100 do
		local star={x=rnd(127),y=rnd(127)}
		add(stars,star)
	end
end

	
